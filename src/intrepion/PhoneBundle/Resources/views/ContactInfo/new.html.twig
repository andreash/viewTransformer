{% extends '::base.html.twig' %}

{% block body -%}
    <h1>ContactInfo creation</h1>

    <form action="{{ path('contactinfo_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form.home) }}
        {{ form_widget(form.cell) }}
        {{ form_widget(form.work) }}
        {{ form_widget(form.fax) }}
        {{ form_rest(form) }}
        <p>
            <button type="submit">Create</button>
        </p>
    </form>

        <ul class="record_actions">
    <li>
        <a href="{{ path('contactinfo') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}
